@model NewsAdministration.Category

@{
    ViewData["Title"] = "Category Details";
}

<h3 class="mt-3">Category Details</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2 font-weight-bold">
            Category Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CategoryName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.CategoryId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
